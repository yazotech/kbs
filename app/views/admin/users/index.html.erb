<h2>用户列表</h2>

<div class='toolbar'>
  <ul class='actions'>
    <li>
  <%= link_to_with_icon 'add', '新建用户', new_admin_user_path, :remote => true %>
</li>
</ul>
</div>
<table class='data-list'>
  <thead>
  <tr>
    <th>用户编号</th>
    <th>登录名称</th>
    <th>邮箱</th>
    <th>公司</th>
    <th>登录次数</th>
    <th>当前登录时间</th>
    <th>最后登录时间</th>
    <th>当前登录IP</th>
    <th>最后登录IP</th>
    <th>&nbsp;</th>
  </tr>
</thead>
<tbody>
  <% @collection.each do |client| %>
   <%= render :partial =>'row', :locals => {:obj => client} %>
  <% end %>
</tbody>
</table>
<%= will_paginate(@collection) %>

<div class='dialog-window'></div>
<br />



