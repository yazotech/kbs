<h1>Listing channels</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Channel number</th>
    <th>Title</th>
    <th>Description</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @channels.each do |channel| %>
  <tr>
    <td><%= channel.company_id %></td>
    <td><%= channel.channel_number %></td>
    <td><%= channel.title %></td>
    <td><%= channel.description %></td>
    <td><%= channel.status %></td>
    <td><%= link_to 'Show', admin_channel_path(channel) %></td>
    <td><%= link_to 'Edit', edit_admin_channel_path(channel) %></td>
    <td><%= link_to 'Destroy', admin_channel_path(channel), confirm: 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Channel', new_admin_channel_path %>
