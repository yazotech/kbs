<h2>协议列表</h2>
<div class='toolbar'>
  <ul class='actions'>
    <li><%= link_to_with_icon 'add', '新建协议', new_admin_agreement_path, :remote => true %>
    </li>
  </ul>
</div>
<%= will_paginate(@collection) %>
<table class='data-list'>
  <thead>
  <tr>
    <th>公司编号</th>
    <th>客户</th>
    <th>产品</th>
    <th>协议开始时间</th>
    <th>协议结束时间</th>
    <th>协议注册时间</th>
    <th>价格</th>
    <th>状态</th>
    <th>修改时间</th>
    <th></th>
  </tr>
  </thead>
  <tbody>

  <% @collection.each do |agreement| %>
   <%= render :partial =>'row', :locals => {:obj => agreement} %>
  <% end %>
  </tbody>
</table>
<%= will_paginate(@collection) %>

<div class='dialog-window'></div>
<br />



